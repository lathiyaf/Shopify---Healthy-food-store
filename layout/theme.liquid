    <script  type="text/javascript" defer>
        window.onscroll = function() {scrollFunction()};
        function scrollFunction() {
            if (document.body.scrollTop > 700 || document.documentElement.scrollTop > 700) {
                document.getElementById("product_bar_fixed").style.bottom = "0";
                $("body").addClass("sticky_bar");
                
            } else {
                document.getElementById("product_bar_fixed").style.bottom = "-100%";
                $("body").removeClass("sticky_bar");
            }
        }

        
    </script>
    
