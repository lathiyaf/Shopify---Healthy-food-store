<script>
 setTimeout(function(){
    $('input:radio').change(function() {
        var valu_prod =  $(this).attr('data-vid');
        var Input_name =  $(this).parents('form').find('.rc_widget__option__selector > input').attr('name');
        var form_find  = $(this).parents('.product-detail-content-inner').find('.form_2_div');
        var patents = $(this).parents('form');
        // console.log('chirag--',Input_name);
        $(patents).find('#sub_product').attr('name',Input_name);
        var prod_id = {{SubProduct_id}};
        if(valu_prod == prod_id) {
            $(form_find).show();
            $(patents).find('.btn_div_1').hide();
            $(patents).find('.rc-radio').removeClass('rc-option--active');
            $(patents).find('.rc-selling-plans').hide();
        } else {
            $(form_find).hide();
            $(patents).find('.btn_div_1').show();
        }
    });

    $('.rc_widget__option__selector > input:radio').change(function() {
      var parents = $(this).parents('form');
      var PriceSub = $(this).parents('.rc_widget__option__selector').find('.rc-option__price').text();
      // console.log('chriag---',PriceSub);
      $(parents).find('.add-price > span').text(PriceSub);
    }); 
}, 5000);
</script>
