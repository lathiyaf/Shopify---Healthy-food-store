                                        <button class="btn btn-link display-3" data-toggle="collapse" data-target="#bonus-collapse--{{ forloop.index }}"><i class="fa fa-plus"></i>{{ block.settings.faq_title }}</button>                                  
                                    </div>
                                    <div id="bonus-collapse--{{ forloop.index }}" class="collapse" aria-labelledby="bonus--{{ forloop.index }}" data-parent="#bonus-program">
                                        <div class="card-body">
                                            {{ block.settings.faq_info }}
                                        </div>
                                    </div>
                                </div>
                            {% endif %}
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
